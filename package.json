{
  "name": "@tyankatsu0105/eslint-plugin",
  "version": "0.10.2",
  "description": "ESLint rules with typescript",
  "keywords": [],
  "homepage": "https://github.com/tyankatsu0105/eslint-plugin#readme",
  "bugs": {
    "url": "https://github.com/tyankatsu0105/eslint-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tyankatsu0105/eslint-plugin.git"
  },
  "license": "MIT",
  "author": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.production.json",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger",
    "test": "mocha --require ts-node/register tests/lib/**/*.spec.ts",
    "sandbox": "eslint ./sandbox/**/*  --rulesdir dist/rules"
  },
  "lint-staged": {
    "*.md": [
      "prettier --write"
    ],
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@mysticatea/eslint-plugin": "^13.0.0",
    "@types/chai": "^4.2.11",
    "@types/eslint": "^6.8.0",
    "@types/mocha": "^7.0.2",
    "@types/react": "^16.9.34",
    "chai": "^4.2.0",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "lint-staged": "^10.1.7",
    "mocha": "^7.1.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "shipjs": "0.18.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": "^10.13.0 || ^12.13.0 || >=14.0.0"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0"
  }
}
